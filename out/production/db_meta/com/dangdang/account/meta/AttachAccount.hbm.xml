<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.account.meta.AttachAccount" table="attach_account" schema="" catalog="account">
        <id name="attachAccountId" column="attach_account_id"/>
        <property name="masterAccountId" column="master_account_id"/>
        <property name="custId" column="cust_id"/>
        <property name="accountGrade" column="account_grade"/>
        <property name="accountIntegral" column="account_integral"/>
        <property name="attachAccountMoney" column="attach_account_money"/>
        <property name="attachAccountMoneyIos" column="attach_account_money_ios"/>
        <property name="attachModifiedVersion" column="attach_modified_version"/>
        <property name="lastModifiedDate" column="last_modified_date"/>
        <property name="lastModifiedSource" column="last_modified_source"/>
    </class>
</hibernate-mapping>
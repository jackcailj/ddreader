<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.account.meta.AccountConsumeItems" table="account_consume_items" schema=""
           catalog="account">
        <id name="consumeItemId" column="consume_item_id"/>
        <property name="custId" column="cust_id"/>
        <property name="consumeNo" column="consume_no"/>
        <property name="consumeMoney" column="consume_money"/>
        <property name="accountType" column="account_type"/>
        <property name="consumeType" column="consume_type"/>
        <property name="creationDate" column="creation_date"/>
        <property name="lastModifyedDate" column="last_modifyed_date"/>
        <property name="consumeSource" column="consume_source"/>
        <property name="deviceType" column="device_type"/>
        <property name="consumeResult" column="consume_result"/>
        <property name="platformNo" column="platform_no"/>
        <property name="sourceOrderNo" column="source_order_no"/>
        <property name="attachAccountItemsId" column="attach_account_items_id"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaActivitySale" table="media_activity_sale" schema="" catalog="digital">
        <id name="activitySaleId" column="activity_sale_id"/>
        <property name="activityId" column="activity_id"/>
        <property name="saleId" column="sale_id"/>
        <property name="startTime" column="start_time"/>
        <property name="endTime" column="end_time"/>
        <property name="sort" column="sort"/>
        <property name="status" column="status"/>
        <property name="salePrice" column="sale_price"/>
    </class>
</hibernate-mapping>
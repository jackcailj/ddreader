<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaMessage" table="media_message" schema="" catalog="digital">
        <id name="messageId" column="message_id"/>
        <property name="receiverCustId" column="receiver_cust_id"/>
        <property name="senderCustId" column="sender_cust_id"/>
        <property name="appId" column="app_id"/>
        <property name="title" column="title"/>
        <property name="description" column="description"/>
        <property name="openDate" column="open_date"/>
        <property name="creationDate" column="creation_date"/>
        <property name="openShareType" column="open_share_type"/>
        <property name="isRead" column="is_read"/>
        <property name="amount" column="amount"/>
        <property name="deviceType" column="device_type"/>
        <property name="isAllDevice" column="is_all_device"/>
        <property name="customContent" column="custom_content"/>
    </class>
</hibernate-mapping>
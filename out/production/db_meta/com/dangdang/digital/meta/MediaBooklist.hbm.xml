<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaBooklist" table="media_booklist" schema="" catalog="digital">
        <id name="booklistId" column="booklist_id"/>
        <property name="channelId" column="channel_id"/>
        <property name="owner" column="owner"/>
        <property name="isShow" column="is_show"/>
        <property name="name" column="name"/>
        <property name="imageUrl" column="image_url"/>
        <property name="description" column="description"/>
        <property name="status" column="status"/>
        <property name="creationDate" column="creation_date"/>
        <property name="creator" column="creator"/>
        <property name="lastUpdateTime" column="last_update_time"/>
        <property name="modifier" column="modifier"/>
        <property name="bookNum" column="book_num"/>
        <property name="storeNum" column="store_num"/>
        <property name="changeNum" column="change_num"/>
    </class>
</hibernate-mapping>
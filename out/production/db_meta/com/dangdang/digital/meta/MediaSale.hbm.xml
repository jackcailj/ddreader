<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaSale" table="media_sale" schema="" catalog="digital">
        <id name="saleId" column="sale_id"/>
        <property name="name" column="name"/>
        <property name="description" column="description"/>
        <property name="type" column="type"/>
        <property name="price" column="price"/>
        <property name="creator" column="creator"/>
        <property name="creationDate" column="creation_date"/>
        <property name="modifier" column="modifier"/>
        <property name="lastModifiedDate" column="last_modified_date"/>
        <property name="isSupportSubscribe" column="is_support_subscribe"/>
        <property name="coverPic" column="cover_pic"/>
        <property name="shelfStatus" column="shelf_status"/>
        <property name="isSupportFullBuy" column="is_support_full_buy"/>
        <property name="isFull" column="is_full"/>
    </class>
</hibernate-mapping>
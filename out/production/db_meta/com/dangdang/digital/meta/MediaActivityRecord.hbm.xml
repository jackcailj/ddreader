<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaActivityRecord" table="media_activity_record" schema=""
           catalog="digital">
        <id name="mediaActivityRecordId" column="media_activity_record_id"/>
        <property name="custId" column="cust_id"/>
        <property name="username" column="username"/>
        <property name="prizeId" column="prize_id"/>
        <property name="activityType" column="activity_type"/>
        <property name="prizeType" column="prize_type"/>
        <property name="prizeVestType" column="prize_vest_type"/>
        <property name="prizeName" column="prize_name"/>
        <property name="worshipedCustId" column="worshiped_cust_id"/>
        <property name="worshipedUsername" column="worshiped_username"/>
        <property name="creationDate" column="creation_date"/>
        <property name="amount" column="amount"/>
    </class>
</hibernate-mapping>
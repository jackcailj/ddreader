<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaChapter" table="media_chapter" schema="" catalog="digital">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="subTitle" column="sub_title"/>
        <property name="indexOrder" column="index_order"/>
        <property name="descs" column="descs"/>
        <property name="wordCnt" column="word_cnt"/>
        <property name="price" column="price"/>
        <property name="iosPrice" column="ios_price"/>
        <property name="creationDate" column="creation_date"/>
        <property name="creator" column="creator"/>
        <property name="lastModifyedDate" column="last_modifyed_date"/>
        <property name="modifier" column="modifier"/>
        <property name="isFree" column="is_free"/>
        <property name="signIds" column="sign_ids"/>
        <property name="signNames" column="sign_names"/>
        <property name="recommandWords" column="recommand_words"/>
        <property name="volumeId" column="volume_id"/>
        <property name="mediaId" column="media_id"/>
        <property name="isShow" column="is_show"/>
        <property name="filePath" column="file_path"/>
        <property name="encrypkey" column="encrypkey"/>
        <property name="encryptVersion" column="encrypt_version"/>
        <property name="volumeName" column="volume_name"/>
        <property name="duration" column="duration"/>
        <property name="upRation" column="up_ration"/>
        <property name="downRation" column="down_ration"/>
    </class>
</hibernate-mapping>
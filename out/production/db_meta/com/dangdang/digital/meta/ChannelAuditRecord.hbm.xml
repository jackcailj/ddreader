<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.ChannelAuditRecord" table="channel_audit_record" schema=""
           catalog="digital">
        <id name="recordId" column="record_id"/>
        <property name="channelId" column="channel_id"/>
        <property name="channelTitle" column="channel_title"/>
        <property name="createDate" column="create_date"/>
        <property name="auditDate" column="audit_date"/>
        <property name="status" column="status"/>
        <property name="failMsg" column="fail_msg"/>
        <property name="auditor" column="auditor"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaConsumerDeposit" table="media_consumer_deposit" schema=""
           catalog="digital">
        <id name="consumerDepositId" column="consumer_deposit_id"/>
        <property name="custId" column="cust_id"/>
        <property name="userName" column="user_name"/>
        <property name="depositNo" column="deposit_no"/>
        <property name="depositOrderNo" column="deposit_order_no"/>
        <property name="isPaid" column="is_paid"/>
        <property name="payment" column="payment"/>
        <property name="relationProductId" column="relation_product_id"/>
        <property name="money" column="money"/>
        <property name="mainGold" column="main_gold"/>
        <property name="giveMainGold" column="give_main_gold"/>
        <property name="subGold" column="sub_gold"/>
        <property name="activeId" column="active_id"/>
        <property name="activeName" column="active_name"/>
        <property name="exchangeRatio" column="exchange_ratio"/>
        <property name="depositSerialNo" column="deposit_serial_no"/>
        <property name="deviceType" column="device_type"/>
        <property name="payTime" column="pay_time"/>
        <property name="creationDate" column="creation_date"/>
        <property name="lastModifiedDate" column="last_modified_date"/>
        <property name="fromPaltform" column="from_paltform"/>
        <property name="productCount" column="product_count"/>
        <property name="isSandbox" column="is_sandbox"/>
    </class>
</hibernate-mapping>
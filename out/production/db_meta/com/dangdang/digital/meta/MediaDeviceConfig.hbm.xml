<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaDeviceConfig" table="media_device_config" schema="" catalog="digital">
        <id name="mediaDeviceId" column="media_device_id"/>
        <property name="deviceTypeId" column="device_type_id"/>
        <property name="deviceTypeCode" column="device_type_code"/>
        <property name="deviceTypeName" column="device_type_name"/>
        <property name="createTime" column="create_time"/>
        <property name="mediaId" column="media_id"/>
    </class>
</hibernate-mapping>
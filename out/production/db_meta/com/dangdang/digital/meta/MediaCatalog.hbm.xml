<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaCatalog" table="media_catalog" schema="" catalog="digital">
        <id name="mcId" column="mc_id"/>
        <property name="productId" column="product_id"/>
        <property name="catalogName" column="catalog_name"/>
        <property name="catalogContent" column="catalog_content"/>
    </class>
</hibernate-mapping>
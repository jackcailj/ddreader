<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaResource" table="media_resource" schema="" catalog="digital">
        <id name="fileId" column="file_id"/>
        <property name="cdnPath" column="cdn_path"/>
        <property name="fileName" column="file_name"/>
        <property name="fileSize" column="file_size"/>
        <property name="uploadDate" column="upload_date"/>
        <property name="dirId" column="dir_id"/>
        <property name="uploader" column="uploader"/>
    </class>
</hibernate-mapping>
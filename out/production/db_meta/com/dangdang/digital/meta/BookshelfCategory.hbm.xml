<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.BookshelfCategory" table="bookshelf_category" schema="" catalog="digital">
        <id name="cId" column="c_id"/>
        <property name="custId" column="cust_id"/>
        <property name="lastChangedDate" column="last_changed_date"/>
        <property name="categoryName" column="category_name"/>
    </class>
</hibernate-mapping>
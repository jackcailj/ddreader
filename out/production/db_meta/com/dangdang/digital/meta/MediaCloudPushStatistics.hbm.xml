<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaCloudPushStatistics" table="media_cloud_push_statistics" schema=""
           catalog="digital">
        <id name="cloudPushStatisticsId" column="cloud_push_statistics_id"/>
        <property name="cloudPushPlanId" column="cloud_push_plan_id"/>
        <property name="pushedNumber" column="pushed_number"/>
        <property name="openedNumber" column="opened_number"/>
        <property name="statisticDay" column="statistic_day"/>
    </class>
</hibernate-mapping>
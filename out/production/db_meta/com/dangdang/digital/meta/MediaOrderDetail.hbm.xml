<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaOrderDetail" table="media_order_detail" schema="" catalog="digital">
        <id name="orderDetailId" column="order_detail_id"/>
        <property name="orderNo" column="order_no"/>
        <property name="saleInfoId" column="sale_info_id"/>
        <property name="saleInfoName" column="sale_info_name"/>
        <property name="activeId" column="active_id"/>
        <property name="totalPrice" column="total_price"/>
        <property name="payMainPrice" column="pay_main_price"/>
        <property name="paySubPrice" column="pay_sub_price"/>
        <property name="prePrice" column="pre_price"/>
        <property name="vspPrice" column="vsp_price"/>
        <property name="couponPrice" column="coupon_price"/>
        <property name="givingPoint" column="giving_point"/>
        <property name="downRationPrice" column="down_ration_price"/>
    </class>
</hibernate-mapping>
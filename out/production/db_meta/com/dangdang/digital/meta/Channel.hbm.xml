<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.Channel" table="channel" schema="" catalog="digital">
        <id name="channelId" column="channel_id"/>
        <property name="ownerId" column="owner_id"/>
        <property name="custId" column="cust_id"/>
        <property name="title" column="title"/>
        <property name="icon" column="icon"/>
        <property name="description" column="description"/>
        <property name="subNumber" column="sub_number"/>
        <property name="status" column="status"/>
        <property name="createDate" column="create_date"/>
        <property name="modifyDate" column="modify_date"/>
        <property name="isCompleted" column="is_completed"/>
        <property name="modifyTimes" column="modify_times"/>
        <property name="shelfStatus" column="shelf_status"/>
        <property name="applyTimes" column="apply_times"/>
        <property name="auditResult" column="audit_result"/>
        <property name="auditDate" column="audit_date"/>
        <property name="auditor" column="auditor"/>
        <property name="isRead" column="is_read"/>
    </class>
</hibernate-mapping>
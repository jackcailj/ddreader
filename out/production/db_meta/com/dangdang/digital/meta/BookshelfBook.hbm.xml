<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.BookshelfBook" table="bookshelf_book" schema="" catalog="digital">
        <id name="bbId" column="bb_id"/>
        <property name="custId" column="cust_id"/>
        <property name="lastChangedDate" column="last_changed_date"/>
        <property name="productId" column="product_id"/>
        <property name="categoryId" column="category_id"/>
    </class>
</hibernate-mapping>
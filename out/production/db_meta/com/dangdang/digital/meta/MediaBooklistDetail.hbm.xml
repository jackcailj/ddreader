<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaBooklistDetail" table="media_booklist_detail" schema=""
           catalog="digital">
        <id name="booklistDetailId" column="booklist_detail_id"/>
        <property name="booklistId" column="booklist_id"/>
        <property name="type" column="type"/>
        <property name="saleId" column="sale_id"/>
        <property name="mediaId" column="media_id"/>
        <property name="productId" column="product_id"/>
        <property name="indexnum" column="indexnum"/>
    </class>
</hibernate-mapping>
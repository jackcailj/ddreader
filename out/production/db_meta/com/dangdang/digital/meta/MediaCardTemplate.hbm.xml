<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaCardTemplate" table="media_card_template" schema="" catalog="digital">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="remark" column="remark"/>
        <property name="type" column="type"/>
        <property name="pic1Path" column="pic1_path"/>
        <property name="pic2Path" column="pic2_path"/>
        <property name="pic3Path" column="pic3_path"/>
    </class>
</hibernate-mapping>
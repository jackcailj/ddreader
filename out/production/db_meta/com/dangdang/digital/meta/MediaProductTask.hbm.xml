<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaProductTask" table="media_product_task" schema="" catalog="digital">
        <id name="id" column="ID"/>
        <property name="ebooklistId" column="EBOOKLIST_ID"/>
        <property name="processor" column="PROCESSOR"/>
        <property name="creator" column="CREATOR"/>
        <property name="created" column="CREATED"/>
        <property name="updator" column="UPDATOR"/>
        <property name="updated" column="UPDATED"/>
        <property name="processingResult" column="PROCESSING_RESULT"/>
        <property name="trialPercentage" column="TRIAL_PERCENTAGE"/>
        <property name="descn" column="DESCN"/>
        <property name="status" column="STATUS"/>
        <property name="category" column="CATEGORY"/>
        <property name="paperBookStatus" column="PAPER_BOOK_STATUS"/>
        <property name="name" column="NAME"/>
        <property name="thirdPartyPermission" column="THIRD_PARTY_PERMISSION"/>
    </class>
</hibernate-mapping>
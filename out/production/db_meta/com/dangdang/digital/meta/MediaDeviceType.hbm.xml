<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaDeviceType" table="media_device_type" schema="" catalog="digital">
        <id name="deviceTypeId" column="device_type_id"/>
        <property name="code" column="code"/>
        <property name="name" column="name"/>
        <property name="desc" column="desc"/>
        <property name="defaultType" column="default_type"/>
        <property name="compatibleServerVersion" column="compatible_server_version"/>
        <property name="binaryValue" column="binary_value"/>
        <property name="sort" column="sort"/>
    </class>
</hibernate-mapping>
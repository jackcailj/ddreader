<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaVolume" table="media_volume" schema="" catalog="digital">
        <id name="volumeId" column="volume_id"/>
        <property name="title" column="title"/>
        <property name="volumeOrder" column="volume_order"/>
        <property name="creationDate" column="creation_date"/>
        <property name="descs" column="descs"/>
        <property name="mediaId" column="media_id"/>
        <property name="cpCode" column="cp_code"/>
        <property name="cpVolumeId" column="cp_volume_id"/>
    </class>
</hibernate-mapping>
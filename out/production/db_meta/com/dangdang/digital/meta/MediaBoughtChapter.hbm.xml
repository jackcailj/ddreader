<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaBoughtChapter" table="media_bought_chapter" schema=""
           catalog="digital">
        <id name="boughtChapterId" column="bought_chapter_id"/>
        <property name="boughtDetailId" column="bought_detail_id"/>
        <property name="chapterId" column="chapter_id"/>
        <property name="chapterTitle" column="chapter_title"/>
        <property name="chapterIndex" column="chapter_index"/>
        <property name="payMainPrice" column="pay_main_price"/>
        <property name="paySubPrice" column="pay_sub_price"/>
    </class>
</hibernate-mapping>
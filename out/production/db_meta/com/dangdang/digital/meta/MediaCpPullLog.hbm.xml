<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaCpPullLog" table="media_cp_pull_log" schema="" catalog="digital">
        <id name="id" column="id"/>
        <property name="cpCode" column="cp_code"/>
        <property name="logType" column="log_type"/>
        <property name="startDate" column="start_date"/>
        <property name="endDate" column="end_date"/>
        <property name="msg" column="msg"/>
        <property name="flag" column="flag"/>
        <property name="batchId" column="batch_id"/>
    </class>
</hibernate-mapping>
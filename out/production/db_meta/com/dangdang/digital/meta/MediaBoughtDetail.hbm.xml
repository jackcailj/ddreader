<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaBoughtDetail" table="media_bought_detail" schema="" catalog="digital">
        <id name="boughtDetailId" column="bought_detail_id"/>
        <property name="boughtId" column="bought_id"/>
        <property name="orderNo" column="order_no"/>
        <property name="orderTime" column="order_time"/>
        <property name="orderContent" column="order_content"/>
        <property name="orderType" column="order_type"/>
        <property name="payMainPrice" column="pay_main_price"/>
        <property name="paySubPrice" column="pay_sub_price"/>
        <property name="chapterId" column="chapter_id"/>
        <property name="boughtType" column="bought_type"/>
        <property name="mediaTitle" column="media_title"/>
        <property name="authorPenName" column="author_pen_name"/>
        <property name="mediaId" column="media_id"/>
        <property name="unitPrice" column="unit_price"/>
    </class>
</hibernate-mapping>
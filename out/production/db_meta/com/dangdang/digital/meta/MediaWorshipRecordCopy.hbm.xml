<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaWorshipRecordCopy" table="media_worship_record_copy" schema=""
           catalog="digital">
        <id name="recordId" column="record_id"/>
        <property name="custId" column="cust_id"/>
        <property name="username" column="username"/>
        <property name="worshipedCustId" column="worshiped_cust_id"/>
        <property name="worshipedUsername" column="worshiped_username"/>
        <property name="creationDate" column="creation_date"/>
        <property name="lastTime" column="last_time"/>
        <property name="amount" column="amount"/>
        <property name="times" column="times"/>
        <property name="sum" column="sum"/>
    </class>
</hibernate-mapping>
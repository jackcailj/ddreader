<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.digital.meta.MediaUserAuthority" table="media_user_authority" schema=""
           catalog="digital">
        <id name="userAuthorityId" column="user_authority_id"/>
        <property name="custId" column="cust_id"/>
        <property name="mediaType" column="media_type"/>
        <property name="authorityType" column="authority_type"/>
        <property name="mediaId" column="media_id"/>
        <property name="authorityStart" column="authority_start"/>
        <property name="authorityEnd" column="authority_end"/>
        <property name="creationDate" column="creation_date"/>
        <property name="lastModifiedDate" column="last_modified_date"/>
        <property name="payMainPrice" column="pay_main_price"/>
        <property name="paySubPrice" column="pay_sub_price"/>
    </class>
</hibernate-mapping>
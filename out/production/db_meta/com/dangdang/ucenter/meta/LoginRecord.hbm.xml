<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.ucenter.meta.LoginRecord" table="login_record" schema="" catalog="ucenter">
        <id name="lrId" column="lr_id"/>
        <property name="custId" column="cust_id"/>
        <property name="custNickname" column="cust_nickname"/>
        <property name="custImg" column="cust_img"/>
        <property name="deviceNo" column="device_no"/>
        <property name="deviceType" column="device_type"/>
        <property name="loginToken" column="login_token"/>
        <property name="createDate" column="create_date"/>
        <property name="lastUpdateDate" column="last_update_date"/>
        <property name="introduct" column="introduct"/>
        <property name="loginPlatform" column="login_platform"/>
        <property name="loginClient" column="login_client"/>
        <property name="gender" column="gender"/>
        <property name="bindPhoneNum" column="bind_phone_num"/>
        <property name="displayId" column="display_id"/>
    </class>
</hibernate-mapping>
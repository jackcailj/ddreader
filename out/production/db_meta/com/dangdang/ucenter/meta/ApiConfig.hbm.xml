<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.ucenter.meta.ApiConfig" table="api_config" schema="" catalog="ucenter">
        <id name="id" column="ID"/>
        <property name="api" column="API"/>
        <property name="name" column="NAME"/>
        <property name="descn" column="DESCN"/>
        <property name="devStatus" column="DEV_STATUS"/>
        <property name="dummyData" column="DUMMY_DATA"/>
        <property name="recentLog" column="RECENT_LOG"/>
        <property name="actionId" column="ACTION_ID"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dangdang.ucenter.meta.TouristLoginInfo" table="tourist_login_info" schema="" catalog="ucenter">
        <id name="id" column="id"/>
        <property name="deviceNo" column="device_no"/>
        <property name="loginUsername" column="login_username"/>
        <property name="loginPwd" column="login_pwd"/>
        <property name="loginTime" column="login_time"/>
        <property name="errorPwd" column="error_pwd"/>
    </class>
</hibernate-mapping>